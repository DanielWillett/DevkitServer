{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "https://github.com/DanielWillett/DevkitServer/tree/master/Module/Schemas/client_config_schema.json",
  "title": "DevkitServer Configuration (Client)",
  "description": "Defines general client-side settings for DevkitServer.",
  "type": "object",
  "properties": {
    "ansi_log": {
      "description": "Add a log to Unturned/Logs that includes whatever visual ANSI codes are used to color the console.",
      "type": "boolean"
    },
    "terminal_full_rgb_support": {
      "description": "Full (24-bit RGB) console color support using virtual terminal sequences (works in the Windows command prompt and most web terminals).",
      "type": "boolean"
    },
    "terminal_virtual_sequence_support": {
      "description": "Basic (4-bit) console color support using virtual terminal sequences (works in most terminals).",
      "type": "boolean"
    },
    "debug_logging": {
      "description": "Adds extra debug logging from DevkitServer. Helps with diagnosing problems but could lower performance.",
      "type": "boolean"
    },
    "hide_map_name": {
      "description": "Hide your current map name from Steam friends list and Discord rich presence. Servers can optionally force this to true.",
      "type": "boolean"
    },
    "disable_terminal": {
      "description": "On Windows, disables opening the terminal window when you launch the game.",
      "type": "boolean"
    },
    "walmart_pc_support": {
      "description": "Removes cosmetic improvements.",
      "type": "boolean"
    },
    "use_vanilla_cartography_in_singleplayer": {
      "description": "Falls back to the vanilla chart and map functions when in singleplayer edit mode.",
      "type": "boolean"
    },
    "enable_object_ui_extension": {
      "description": "Enables a preview window for your selected object in the Object Editor.",
      "type": "boolean"
    },
    "enable_better_map_creation": {
      "description": "Enables an override for better initial map creation (adding a new map).",
      "type": "boolean"
    },
