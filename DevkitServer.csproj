<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>net481</TargetFramework>
    <AssemblyTitle>DevkitServer</AssemblyTitle>
    <Product>DevkitServer</Product>
    <Copyright>Copyright ©  2023</Copyright>
    <OutputPath>bin\$(Configuration)\</OutputPath>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
    <AppendTargetFrameworkToOutputPath>false</AppendTargetFrameworkToOutputPath>
    <AppendRuntimeIdentifierToOutputPath>false</AppendRuntimeIdentifierToOutputPath>
    <RepositoryUrl>https://github.com/DanielWillett/DevkitServer</RepositoryUrl>
    <Configurations>ClientDebug;ClientRelease;ServerDebug;ServerRelease</Configurations>
    <AllowUnsafeBlocks>True</AllowUnsafeBlocks>
    <ImplicitUsings>enable</ImplicitUsings>
    <GenerateDocumentationFile>True</GenerateDocumentationFile>
    <NoWarn>1701;1702,1591,1587,1711,1735,1573,1574,1570,1584,1658</NoWarn>
  </PropertyGroup>
  <PropertyGroup>
    <DisableImplicitFrameworkReferences>true</DisableImplicitFrameworkReferences>
  </PropertyGroup>
  <Import Project="$(SolutionDir)Common.targets" Condition="exists('$(SolutionDir)Common.targets')" />
  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="SET &quot;sourcedir=$(OutDir)&#xD;&#xA;SET &quot;config=$(ConfigurationName)&#xD;&#xA;ECHO Building %25config%25&#xD;&#xA;IF %25config%25 == ClientDebug (&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Client.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Client.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Client.xml*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Client.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Client.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Client.xml*&quot;&#xD;&#xA;) ELSE IF %25config%25 == ClientRelease (&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Client.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Client.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Client.xml*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Client.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Client.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Client.xml*&quot;&#xD;&#xA;) ELSE IF %25config%25 == ServerDebug (&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Server.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Server.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Server.xml*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Server.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Server.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Server.xml*&quot;&#xD;&#xA;) ELSE IF %25config%25 == ServerRelease (&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Server.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Server.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\Program Files (x86)\Steam\steamapps\common\Unturned\Modules\DevkitServer\Bin\$(ProjectName)_Server.xml*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).dll*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Server.dll*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).pdb*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Server.pdb*&quot;&#xD;&#xA; XCOPY /v /y &quot;%25sourcedir%25\$(ProjectName).xml*&quot; &quot;C:\SteamCMD\steamapps\common\U3DS\Modules\DevkitServer\Bin\$(ProjectName)_Server.xml*&quot;&#xD;&#xA;) ELSE (&#xD;&#xA; ECHO No files copied.&#xD;&#xA;)" />
  </Target>
  <ItemGroup>
    <Compile Remove="DevkitServer.Tests\**" />
    <Compile Remove="packages\**" />
    <Compile Remove="TestPlugin\**" />
    <EmbeddedResource Remove="DevkitServer.Tests\**" />
    <EmbeddedResource Remove="packages\**" />
    <EmbeddedResource Remove="TestPlugin\**" />
    <None Remove="DevkitServer.Tests\**" />
    <None Remove="packages\**" />
    <None Remove="TestPlugin\**" />
  </ItemGroup>
  <ItemGroup>
    <None Remove="Common.targets" />
  </ItemGroup>
  <ItemGroup>
    <Using Remove="System.Net.Http" />
    <Using Remove="System.Text" />
    <PackageReference Include="Newtonsoft.Json" Version="13.0.3" />
    <Using Include="SDG.NetTransport" />
    <Using Include="SDG.Unturned" />
    <Using Include="Steamworks" />
    <Using Include="System.Collections" />
    <Using Include="UnityEngine" />
    <Using Include="DevkitServer.Util" />
    <Using Include="DevkitServer.Util.Logger">
      <Alias>Logger</Alias>
    </Using>
    <Using Include="UnityEngine.Object">
      <Alias>Object</Alias>
    </Using>
  </ItemGroup>
</Project>